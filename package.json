  },
  "keywords": [
    "assets",
    "css",
    "stylus",
    "logo",
    "favicon"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webuildsg/assets/issues"
  },
  "homepage": "https://github.com/webuildsg/assets#readme",
  "devDependencies": {
    "clean-css": "^3.4.8",
    "stylus": "^0.53.0"
  }
}
